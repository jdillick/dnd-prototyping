    "node-sass": "^4.11.0",
    "node-sass-functions-json": "^1.0.0",
    "node-sass-tilde-importer": "^1.0.2",
    "nodemon": "^1.18.7",
    "ora": "^3.0.0",
    "po2json": "^1.0.0-alpha",
    "readdir-recursive": "0.0.4",
    "slugify": "^1.3.3",
    "stylelint": "^9.10.1",
