    "yargs": "^12.0.1"
  },
  "libDependencies": [
    "react"
  ],
  "nodemonConfig": {
    "watch": [
      "src/manifest.js"
    ],
    "ignore": [
      "src/app/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
